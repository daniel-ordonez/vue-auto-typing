{
  "name": "vue-auto-typing",
  "version": "0.1.0",
  "private": false,
  "author": {
    "name": "daniel ordonez"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "vue component",
    "auto typing"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build-vue": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name VueAutoTyping ./src/components/VueAutoTyping.vue",
    "build-wc": "vue-cli-service build --target wc --name auto-typing ./src/wrapper.js --dest ./web-component"
  },
  "dependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.3",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.3",
    "@vue/cli-service": "^3.0.0-rc.3",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
